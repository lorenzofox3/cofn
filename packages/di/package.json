{
  "name": "@cofn/di",
  "version": "0.0.1",
  "description": "DOM as DI container",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./dist/cofn-di.js"
      }
    }
  },
  "scripts": {
